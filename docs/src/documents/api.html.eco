---
layout: default
---
<% for section, api of @dox: %>
	<h2><%= section %></h2>

	<% for item in api: %>
		<% if not item.ignore and not item.isPrivate and item.ctx: %>
			<p>
				<h3><%= item.ctx.name %></h3>

				<h4><%= item.description.summary %></h4>

				<% for tag in item.tags: %>
					<%= tag.name %><br>
				<% end %>
			</p>
		<% end %>
	<% end %>
<% end %>
